print("Lets find the Greatest Common Factor!")

lst1 = []

num1 = int(input("Please enter a number:"))
for i1 in range (1, num1 + 1):
     if num1 % i1 == 0:
          lst1.append(i1)

lst2 = []

num2 = int(input("Please enter a number:"))
for i2 in range (1, num2 + 1):
     if num2 % i2 == 0:
          lst2.append(i2)

common_factors = []
for a in lst1:
     for b in lst2:
          if (a==b):
               common_factors.append(a)
z = common_factors[-1]
print(z)



